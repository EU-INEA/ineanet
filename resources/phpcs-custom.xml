<?xml version="1.0"?>

<ruleset name="INEA">
    <description>Custom ruleset for subsite usage.</description>

    <!-- Views handlers not strictly follow Drupal class name conventions. -->
    <rule ref="Drupal.Semantics.FunctionT.WhiteSpace">
        <exclude-pattern>**/inea_h2020_ct.features.field_instance.inc</exclude-pattern>
    </rule>

    <!-- Exclude inea_migration, code will be removed in next release. -->
    <exclude-pattern>**/inea_migration</exclude-pattern>

</ruleset>
